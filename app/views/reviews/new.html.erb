<h1>New review</h1>
<%= simple_form_for [@restaurant, @review] do |f| %>
  <%= f.input :content %>
  <%= f.select :rating, options_for_select((0..5))%>
  <%= f.submit "Submit review", class: "btn btn-primary" %>
<% end %>

<%= link_to 'back', restaurant_path(@restaurant), class: "btn btn-primary" %>
